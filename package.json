{
  "name": "nodana",
  "version": "1.0.0",
  "description": "A Node JS Solana CLI",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "clean": "rimraf dist",
    "build:esm": "tsc --project tsconfig.esm.json && tsc-alias -p tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json && tsc-alias -p tsconfig.cjs.json",
    "build": "npm run clean && npm run build:esm && npm run build:cjs"
  },
  "keywords": [
    "solana-cli",
    "typescript-solana-cli",
    "Node JS Solana CLI"
  ],
  "author": "Pintu Das",
  "repository": {
    "type": "git",
    "url": "https://github.com/Shyft-to/js.git"
  },
  "bugs": {
    "url": "https://github.com/Shyft-to/js/issues"
  },
  "homepage": "https://impin2rex.github.io",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.0",
    "jest": "^29.4.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@solana/web3.js": "^1.73.2"
  }
}
